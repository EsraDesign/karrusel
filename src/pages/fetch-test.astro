---
const artists = await fetch("https://fetch-with-astro.netlify.app/api/artists").then(response => response.json());
---

<ul>
    {
        artists.map(artist => (
        <li>
            <h3>{artist.title}</h3>
            <p>{artist.type}</p>
            <img src={artist.og_image.url} alt="billede">
            </li>
            ))
    }
</ul> 